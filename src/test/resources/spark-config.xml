        <!-- report theme -->
        <!-- standard, dark -->
        <theme>standard</theme>
    
        <!-- enables timeline -->
        <!-- defaults to true -->
        <!--  enableTimeline>true</enableTimeline> -->
	
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
